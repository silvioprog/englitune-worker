{
  "name": "englitune-worker",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --var CORS_ORIGIN:\"*\"",
    "deploy": "wrangler deploy --minify",
    "lint": "eslint .",
    "test": "vitest --run",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "hono": "^4.11.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16",
    "wrangler": "^4.57.0"
  }
}
